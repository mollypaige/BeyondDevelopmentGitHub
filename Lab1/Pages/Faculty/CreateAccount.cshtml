@page
@model Lab1.Pages.Faculty.CreateAccountModel
@{
}

<form asp-page="CreateAccount">
    <label>Enter New Username:</label>
    <input asp-for="Username" class="form-control" placeholder="Enter Username" />
    <br />
    <label>Enter New User's Password:</label>
    <input asp-for="Password" class="form-control" type="password" />
    <br />
    <label>Enter New User's First Name:</label>
    <input asp-for="FirstName" class="form-control"/>
    <br />
    <label>Enter New User's Last Name:</label>
    <input asp-for="LastName" class="form-control"/>
    <br />
    <label>Enter New User's Email:</label>
    <input asp-for="Email" class="form-control" />
    <br />
    <input type="submit" class="btn btn-primary" />
</form>
@{
    if (ViewData["UserCreate"] != null)
    {
        <p class="text-success">@ViewData["UserCreate"]</p>
    }

}